<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net5.0</TargetFramework>
        <Title>Bi5.Net</Title>
        <Authors>Tomas Rampas</Authors>
        <Description>Download, convert, and export Dukascopy tick data. Package converts tick data from prorietary 
            Bi5 format to standard CSV file with OHLCV data resampled to custom time frame resultion. </Description>
        <PackageTags>dukascopy bi5 tickdata forex</PackageTags>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <RepositoryUrl>https://github.com/tomas-rampas/Dukas.Net</RepositoryUrl>
        <PackageVersion>0.1.0</PackageVersion>
        <PackageProjectUrl>https://tomas-rampas.github.io/Dukas.Net/</PackageProjectUrl>
    </PropertyGroup>

    <PropertyGroup>
        <PackageLicenseExpression>Unlicense</PackageLicenseExpression>
    </PropertyGroup>
    
    <ItemGroup>
      <PackageReference Include="LZMA-SDK" Version="19.0.0" />
      <PackageReference Include="System.Linq.Async" Version="5.0.0" />
    </ItemGroup>

</Project>
